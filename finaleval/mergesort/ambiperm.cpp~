#include<iostream>
#include<math.h>

using namespace std;
void print_array( int* arr, int n )
{
	for( int i = 1; i <=n; i++ )
	{
		cout << arr[i] << endl;
	}
	cout << endl;
}
int main()
{
	long int n;
	long int number;
	long int* answer;
	while()
	{
		cin >> n;
		answer = new long int( n );
		for( int i = 1; i <= n; i++ )
		{
			cin >> number;
			answer[number] = i;
		}
		print_array( answer, n );
	}
}
